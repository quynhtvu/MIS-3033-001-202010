<Window x:Class="JSON_GOT.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JSON_GOT"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnGenerateQuote" Content="Get Quote" HorizontalAlignment="Left" Height="53" Margin="308,279,0,0" VerticalAlignment="Top" Width="138" Click="btnGenerateQuote_Click"/>
        <Label x:Name="label" Content="Game of Thrones Quotes" HorizontalAlignment="Left" Height="49" Margin="238,40,0,0" VerticalAlignment="Top" Width="308" FontSize="24" FontWeight="Bold" Background="{x:Null}"/>
        <Label x:Name="lblQuote" Content="" HorizontalAlignment="Left" Height="95" Margin="138,139,0,0" VerticalAlignment="Top" Width="490" Background="{x:Null}" Foreground="#FFD94E4E"/>

    </Grid>
</Window>
